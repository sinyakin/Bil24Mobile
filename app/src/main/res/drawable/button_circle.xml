<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

  <item android:state_enabled="false">
    <shape android:shape="oval">
      <stroke
          android:width="1dp"
          android:color="@color/date_bg_border__disable">
      </stroke>

      <solid
          android:color="@color/date_bg_disable">
      </solid>
    </shape>
  </item>

  <item android:state_focused="true">
    <shape android:shape="oval">
      <stroke
          android:width="1dp"
          android:color="@color/date_bg_border">
      </stroke>
      <solid
          android:color="@color/date_bg_press">
      </solid>
    </shape>
  </item>

  <item android:state_pressed="true">
    <shape android:shape="oval">
      <stroke
          android:width="1dp"
          android:color="@color/date_bg_border">
      </stroke>
      <solid
          android:color="@color/date_bg_press">
      </solid>
    </shape>
  </item>

  <item>
    <shape android:shape="oval">

      <stroke
          android:width="1dp"
          android:color="@color/date_bg_border">
      </stroke>

      <solid
          android:color="@color/date_bg">
      </solid>


    </shape>
  </item>

</selector>